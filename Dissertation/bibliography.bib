	month = aug,
	year = {2022},
	pages = {339--355},
	file = {Full Text PDF:/Users/jpsmith/Zotero/storage/E7IMUAGV/Henderson et al. - 2022 - Advanced RFSoC readout for space-based superconduc.pdf:application/pdf;Snapshot:/Users/jpsmith/Zotero/storage/YKJDFPWI/12.2630412.html:text/html},
