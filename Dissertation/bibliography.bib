	author = {Gebauer, Richard and Karcher, Nick and Gusenkova, Daria and Spiecker, Martin and Grünhaupt, Lukas and Takmakov, Ivan and Winkel, Patrick and Planat, Luca and Roch, Nicolas and Wernsdorfer, Wolfgang and Ustinov, Alexey V. and Weber, Marc and Weides, Martin and Pop, Ioan M. and Sander, Oliver},
	year = {2020},
	pages = {020015},
	file = {Gebauer et al. - 2020 - State preparation of a fluxonium qubit with feedba.pdf:/Users/jpsmith/Zotero/storage/MFQCN96T/Gebauer et al. - 2020 - State preparation of a fluxonium qubit with feedba.pdf:application/pdf},
}

